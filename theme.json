{
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"version": 3,
	"settings": {
		"appearanceTools": true,
		"useRootPaddingAwareAlignments": false,
		"layout": {
			"contentSize": "900px",
			"wideSize": "1200px",
			"allowCustomContentAndWideSize": true
		},
		"typography": {
			"dropCap": false,
			"fluid": true,
			"fontFamilies": [
				{
					"fontFace": [
						{
							"fontFamily": "Inter",
							"fontStretch": "normal",
							"fontStyle": "normal",
							"fontWeight": "400",
							"src": [
								"file:./aasets/fonts/inter-v19-latin_latin-ext-regular.woff2'"
							]
						},
						{
							"fontFamily": "Inter",
							"fontStretch": "normal",
							"fontStyle": "itaic",
							"fontWeight": "400",
							"src": [
								"file:./assets/fonts/inter-v19-latin_latin-ext-italic.woff2'"
							]
						},
						{
							"fontFamily": "Inter",
							"fontStretch": "normal",
							"fontStyle": "normal",
							"fontWeight": "700",
							"src": [
								"file:./assets/fonts/inter-v19-latin_latin-ext-700.woff2'"
							]
						}
					],
					"fontFamily": "\"Inter\", sans-serif",
					"name": "Inter",
					"slug": "inter"
				}
			]
		}
	}
}
